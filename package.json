{
  "name": "arjaukraustomes.lt",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "dependencies": {
    "cssnano": "^4.1.8",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "atomizer": "^3.5.2",
    "install": "^0.12.2",
    "npm": "^6.5.0",
    "parcel-bundler": "^1.11.0",
    "postcss-cli": "^6.1.1"
  },
  "scripts": {
    "build:ts:watch": "parcel build src/app.ts --out-dir ./static/js",
    "build:ts": "parcel build --no-source-maps build src/app.ts --out-dir ./static/js",
    "build:atomic-css": "atomizer -c atomizer.config.js -o ./static/css/atomic.css ./index.html",
    "build:css": "npm run-script build:atomic-css && postcss static/css/atomic.css > static/css/style.css",
    "build": "npm run-script build:ts && npm run-script build:css"
  },
  "browserslist": [
    "since 2017-06"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RokasMuningis/arjaukraustomes.lt.git"
  },
